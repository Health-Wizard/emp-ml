model Channels {
  id               Int      @id @default(autoincrement())
  url              String   @unique
  name             String   @unique
  create_timestamp DateTime
  type             String
  member_count     Int
  last_message_id  Int
}

model Employee {
  id            Int      @id @default(autoincrement())
  empId         Int      @unique
  username      String
  name          String
  companyEmail  String
  designation   String
  dateOfJoining DateTime
  salary        String
  role          String
  gender        String
  age           Int
  Answer        Answer[]
  Register      Register @relation(fields: [empId], references: [id])
}

model Register {
  id           Int       @id @default(autoincrement())
  name         String
  username     String    @unique
  password     String
  companyEmail String
  role         String
  Employee     Employee?
}

model Answer {
  id       Int      @id @default(autoincrement())
  qId      Int
  empId    Int
  Employee Employee @relation(fields: [empId], references: [id])
  Question Question @relation(fields: [qId], references: [id])
}

model Question {
  id       Int      @id @default(autoincrement())
  question String
  Answer   Answer[]
}
